<script>
var hasEverShow=new Array();
$(document).ready(function(){
	Array.prototype.contains=function(obj){
	    for(var i=0; i<this.length; i++) {  
	        if(this[i]==obj) {  
	            return true;  
	        }  
	    }  
	    return false;  
	};

	$('.naviSide li').click(function(){
	    var str = $(this).text();
		$('.naviSide').children().removeClass('on');
		$('.naviSide li:nth-child('+str+')').addClass('on');
		$('.box').children().removeClass('on');
		$('.box div:nth-child('+str+')').addClass('on');
		if(hasEverShow.contains(str)==false){
			$('.box div:nth-child('+str+')').find("img").each(function(){
				$(this).attr("src",$(this).attr("init_src"));
				hasEverShow.push(str);
			});
		}

	});
	
});
</script>
